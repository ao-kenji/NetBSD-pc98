#set -x

/bin/ps -x | 
(
	while read PCSD_PID ARG2 ARG3 ARG4 PROG_NAME
	do
		PROG_BASE="${PROG_NAME##*/}"
		TARGET="${PROG_BASE%% *}"
		if [ X$TARGET = "Xpcsd" ]
		then
			echo kill pcsd $PCSD_PID
			/bin/kill -INT $PCSD_PID
			exit 0
		fi
	done
	echo "no pcsd running"
	exit 255
)
